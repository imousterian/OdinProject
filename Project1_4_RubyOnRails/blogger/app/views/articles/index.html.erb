<h1> All Articles </h1>

<%= link_to 'Create a New Article', new_article_path(@article), class: 'new_article' %>

<ul id = "articles">
    <% @articles.each do |article| %>
        <li>
            <%= link_to article.title, article_path(article), class: 'article_title' %>
        </li>
    <% end %>


</ul>


